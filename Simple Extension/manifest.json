{
    "manifest_version": 3,
    "name": "Basic Extension",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": "./assets/icon.png"
    },
    "background": {
      "service_worker" : "background.js"
    },
    "host_permissions":["<all_urls>"],
    "permissions": ["activeTab","scripting"]
  }